    "# Define function to connect to the PostgreSQL database\n",
    "def connect_to_database():\n",
    "    try:\n",
    "        conn = psycopg2.connect(\"dbname='project_3g1_DB' user='postgres' password='postgres' host='localhost'\")\n",
    "        return conn\n",
    "    except psycopg2.Error as e:\n",
    "        print(\"Error connecting to the database:\", e)\n",
    "        return None\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "36da3e8f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define function to execute SQL queries\n",
    "def execute_query(query):\n",
    "    conn = connect_to_database()\n",
    "    if conn:\n",
    "        try:\n",
    "            with conn.cursor() as cursor:\n",
    "                cursor.execute(query)\n",
    "                result = cursor.fetchall()\n",
    "                return result\n",
    "        except psycopg2.Error as e:\n",
    "            print(\"Error executing query:\", e)\n",
    "    return None\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "b30669c1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Route to the homepage\n",
    "@app.route('/')\n",
    "def index():\n",
    "    # Define SQL queries\n",
    "    tumor_query = \"SELECT Histologic_Grade, Tumor_Size_cm FROM cleaned_supplementary\"\n",
    "    age_query = \"SELECT Age_at_diagnosis FROM cleaned_supplementary\"\n",
    "    sex_query = \"SELECT Gender FROM cleaned_supplementary\"\n",
    "    cancer_stage_query = \"SELECT Dukes_Stage FROM cleaned_supplementary\"\n",
    "    lymph_node_involvement_query = \"SELECT Lymphovascular_invasion FROM cleaned_supplementary\"\n",
    "    \n",
    "    # Execute SQL queries\n",
    "    tumor_data = execute_query(tumor_query)\n",
    "    age_data = execute_query(age_query)\n",
    "    sex_data = execute_query(sex_query)\n",
    "    cancer_stage_data = execute_query(cancer_stage_query)\n",
    "    lymph_node_involvement_data = execute_query(lymph_node_involvement_query)\n",
    "    \n",
    "    # Render the template with the data\n",
    "    return render_template('index.html', tumor_data=tumor_data, age_data=age_data, \n",
    "                           sex_data=sex_data, cancer_stage_data=cancer_stage_data,\n",
    "                           lymph_node_involvement_data=lymph_node_involvement_data)\n"
   ]
